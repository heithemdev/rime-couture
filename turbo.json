{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "daemon": false,
  "globalEnv": [
    "NODE_ENV",
    "DATABASE_URL",
    "DIRECT_URL",
    "NODE_TLS_REJECT_UNAUTHORIZED",
    "PG_POOL_MAX",
    "PG_POOL_IDLE_TIMEOUT_MS",
    "PG_POOL_CONN_TIMEOUT_MS",
    "PG_STATEMENT_TIMEOUT_MS",
    "DEBUG_SQL",
    "RESEND_API_KEY",
    "SMTP_HOST",
    "SMTP_PORT",
    "SMTP_SECURE",
    "SMTP_USER",
    "SMTP_PASS",
    "MAIL_FROM",
    "NEXT_PUBLIC_APP_URL",
    "NEXT_PUBLIC_SITE_URL",
    "INTERNAL_APP_URL",
    "GEMINI_API_KEY",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME",
    "NEXT_PUBLIC_CLOUDINARY_API_KEY",
    "CLOUDINARY_API_SECRET",
    "CLOUDINARY_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "DIRECT_URL",
        "NODE_TLS_REJECT_UNAUTHORIZED",
        "PG_POOL_MAX",
        "PG_POOL_IDLE_TIMEOUT_MS",
        "PG_POOL_CONN_TIMEOUT_MS",
        "PG_STATEMENT_TIMEOUT_MS",
        "DEBUG_SQL",
        "RESEND_API_KEY",
        "SMTP_HOST",
        "SMTP_PORT",
        "SMTP_SECURE",
        "SMTP_USER",
        "SMTP_PASS",
        "MAIL_FROM",
        "NEXT_PUBLIC_APP_URL",
        "NEXT_PUBLIC_SITE_URL",
        "INTERNAL_APP_URL",
        "GEMINI_API_KEY",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME",
        "NEXT_PUBLIC_CLOUDINARY_API_KEY",
        "CLOUDINARY_API_SECRET",
        "CLOUDINARY_URL"
      ],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "apps/*/.env*", "packages/*/.env*"],
      "outputs": [".next/**", "dist/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "DIRECT_URL",
        "NODE_TLS_REJECT_UNAUTHORIZED",
        "PG_POOL_MAX",
        "PG_POOL_IDLE_TIMEOUT_MS",
        "PG_POOL_CONN_TIMEOUT_MS",
        "PG_STATEMENT_TIMEOUT_MS",
        "DEBUG_SQL",
        "RESEND_API_KEY",
        "SMTP_HOST",
        "SMTP_PORT",
        "SMTP_SECURE",
        "SMTP_USER",
        "SMTP_PASS",
        "MAIL_FROM",
        "NEXT_PUBLIC_APP_URL",
        "NEXT_PUBLIC_SITE_URL",
        "INTERNAL_APP_URL",
        "GEMINI_API_KEY",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET"
      ]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}